<div class="search-result">
  <div class="top-pagination">
    <%= paginate users %>
  </div>
  <% users.each do |user| %>
      <ul>
        <li><%= user.name %></li>
        <li><%= image_tag get_user_icon(user) %></li>
        <li><%= render 'shared/follows/btn', followed_user: user %></li>
      </ul>
  <% end %>
  <div class="bottom-pagination">
    <%= paginate users %>
  </div>
</div>
