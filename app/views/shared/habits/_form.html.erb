<%= form_with model: @form, url: moved_path do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :name, class: "required" %>
  <%= f.text_field :name, placeholder: "例)ヨガ" %>
  <%= f.label :effects, class: "required" %>
  <%= f.text_field :effects, placeholder: "例)肩こり,体全体" %>
  <%= f.hidden_field :effects_ids %>
  <%= f.label :scheme, class: "required" %>
  <%= f.text_area :scheme, placeholder: "例)一つ一つの動きを意識して、取り組む。" %>
  <%= f.label :urls %>
  <% get_url_max.times do |i| %>
    <%= f.hidden_field :urls_ids, multiple: true %>
    <%= f.text_field :urls, multiple: true, placeholder: "例)https://www.youtube.com/watch?v=e9HR7-3I3MA" %>
  <% end %>


  <%= f.label :period_for_effect %>
  <%= f.text_field :period_for_effect, placeholder: "例)2ヶ月" %>
  <!-- 挑戦中か結果済みか-->
  <%= f.submit '保存する', class: "submit-button"%>
  <%= link_to 'キャンセル', '#', class: "cancel-link" %>
<% end %>
